-- Creating tables for house prices 2015-2019
CREATE TABLE House_Price (
     Region_Name VARCHAR(40) NOT NULL,
     State_Name VARCHAR(40) NOT NULL,
	State_and_County VARCHAR(40) NOT NULL,
	Region_code VARCHAR(10) NOT NULL,
	"2015" FLOAT NOT NULL, 
	"2016" FLOAT NOT NULL,
	"2017" FLOAT NOT NULL,
	"2018" FLOAT NOT NULL,
	"2019" FLOAT NOT NULL, 
	"2020" FLOAT NOT NULL, 
	Percent_Price_change FLOAT NOT NULL,
	
     PRIMARY KEY (Region_code)
);

-- Creating tables for county to county 2015-2019 ins outs net gross 
CREATE TABLE In_Outs_Nets_Gross (
    Region_Code_of_Geography_A VARCHAR(5) NOT NULL,
	State_Code_of_Geography_A VARCHAR(10) NOT NULL, 
    FIPS_County_Code_of_Geography_A VARCHAR(5) NOT NULL,
	Region_Code_Geography_B VARCHAR(5) NOT NULL, 
	State_US_Island_Area_Foreign_Region_Code_of_Geography_B VARCHAR(40) NOT NULL,
	FIPS_County_Code_of_Geography_B VARCHAR(10) NOT NULL,
	State_Name_of_Geography_B VARCHAR(10) NOT NULL,
    County_Name_of_Geography_B VARCHAR(10) NOT NULL,
    State_and_County_of_Geography_B VARCHAR(10) NOT NULL,
    Flow_from_Geography_B_to_Geography_A VARCHAR(40) NOT NULL,
    "Counterflow_from_Geography_A_to_Geography_B" INT NOT NULL,
    Net_Migration_from_Geography_B_to_Geography_A1 INT NOT NULL,
    Gross_Migration_between_Geography_A_and_Geography_B1 INT NOT NULL
);
